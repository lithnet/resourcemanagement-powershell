          <Output />
        </Example>
        <Example>
          <Name>Get all approval requests</Name>
          <Cmd>Get-ApprovalRequest</Cmd>
          <Description />
          <Output />
        </Example>
        <Example>
          <Name>Approval all pending requests</Name>
          <Cmd>Get-ApprovalRequest -Status Pending | Set-PendingApprovalRequest -Decision Approved</Cmd>
